class Solution:
    def sortArrayByParity(self, A: List[int]) -> List[int]:
        B=[]
        C=[]
        for i in A:
            if i%2==0:
                B.append(i)
            else:
                C.append(i)
        for i in range(len(C)):
            B.append(C[i])
        return B
            
