class Solution:
    def judgeCircle(self, moves: str) -> bool:
        count=0
        for i in moves:
            if i=='U':
                count+=2
            elif i=='D':
                count-=2
            elif i=='R':
                count+=1
            else:
                count-=1
        if count==0:
            return True
        else:
            return False
