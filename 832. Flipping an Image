class Solution:
    def flipAndInvertImage(self, A: List[List[int]]) -> List[List[int]]:
        C=[]
        for i in A:
            B=[]
            i=reversed(i)
            for j in i:
                
                if j==1:
                    B.append(0)
                else:
                    B.append(1)
            C.append(B)
        return C    
            
